{
  "presets": [ 
    [ 
      "es2015", { 
        "modules": false
      }
    ]
  ],
  "plugins": [ 
    "transform-object-rest-spread", 
    [ 
      "transform-react-jsx", 
      { 
        "pragma": "h" 
      } 
    ],
    // replace react as it's used by AltContainer
    [
      "module-resolver",
      {
        "alias": {
          "react": "preact-compat"
        }
      }
    ] 
  ]
}